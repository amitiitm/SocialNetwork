<% if @search_type == "email" or @search_type == "name" -%>
	<% if @search_type == "email" -%>
		<% for user in @users -%>
			<%= {:type => @search_type, :text => user.name ,:options => "#{user.name}<br>#{user.email}", :user_id => user.id, :first_name => user.first_name}.to_json %>
		<% end -%>
	<% else %>
		<% if @users.size > 0 -%>
			<% for user in @users -%>
				<%= {:type => @search_type, :text => user.name, :options => user.name, :user_id => user.id, :account_id => user.account_id,  :first_name => user.first_name}.to_json %>
			<% end -%>
		<% end -%>
	<% end -%>
<% else -%>
	<% if @phones.size > 0 -%>
			<% for phone in @phones -%>
				<%= {:type => "phone", :text => phone.number, :options => "#{phone.user.name}<br>#{phone.number}", :user_id => phone.user.id, :phone_id => phone.id,
				:first_name => phone.user.first_name}.to_json %>
			<% end -%>
	<% end -%>
<% end -%>